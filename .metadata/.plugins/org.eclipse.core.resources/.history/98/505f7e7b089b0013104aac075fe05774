
public class StudentTest extends junit.framework.TestCase {
	public void testCreate() {
		final String firstStudentName = "Janes Does";
		final String secondStudentName = "Joe Blows";
		
		Student student = new Student(firstStudentName);
		assertEquals(firstStudentName, student.getName());
		
		Student student2 = new Student(secondStudentName);
		assertEquals(secondStudentName, student2.getName());
		
		assertEquals(secondStudentName, student.getName());
	}

}
